<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>ProjetJEE</display-name>
  <welcome-file-list>
    <welcome-file>JSPAccueil</welcome-file>
  </welcome-file-list>
  <session-config>
    <session-timeout>10</session-timeout>
  </session-config>
  <filter>
    <filter-name>FiltreAuth</filter-name>
    <filter-class>fr.eni.ecole.projet.filtre.FiltreAuth</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>FiltreAuth</filter-name>
    <url-pattern>/mon_compte</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>JSPAccueil</servlet-name>
    <jsp-file>/WEB-INF/jsp/accueil.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>JSPAccueil</servlet-name>
    <url-pattern>/accueil</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>JSPConnexion</servlet-name>
    <jsp-file>/WEB-INF/jsp/connexion.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>JSPConnexion</servlet-name>
    <url-pattern>/connexion</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>JSPInscription</servlet-name>
    <jsp-file>/WEB-INF/jsp/inscription.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>JSPInscription</servlet-name>
    <url-pattern>/inscription</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>JSPMonCompte</servlet-name>
    <jsp-file>/WEB-INF/jsp/monCompte.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>JSPMonCompte</servlet-name>
    <url-pattern>/mon_compte</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>JSPAjouterPlat</servlet-name>
    <jsp-file>/WEB-INF/jsp/ajouterPlat.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>JSPAjouterPlat</servlet-name>
    <url-pattern>/ajouter_plat</url-pattern>
  </servlet-mapping>
</web-app>